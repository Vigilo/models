#: src/vigilo/models/scripts/purge_vigiboard.py:91
#, python-format
msgid ""
"Deleted %(nb_deleted)d closed events which were at least %(days)d days "
"old."
msgstr ""
"Gelöschte %(nb_deleted)d abgeschlossene Ereignisse, die mindestens "
"%(days)d Tage alt sind."

#: src/vigilo/models/scripts/purge_vigiboard.py:104
#, python-format
